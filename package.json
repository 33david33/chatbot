{
  "name": "chatbot",
  "version": "1.0.0",
  "description": "Chatbot",
  "license": "ISC",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "start": "pm2 start chatbot.json",
    "stop": "pm2 stop chatbot.json",
    "reload": "pm2 reload chatbot.json",
    "dev": "npm run build:bff && env-cmd cross-env NODE_ENV=development supervisor --inspect=7070 --watch src -- -r  babel-register-ts  src/bff/server.ts",
    "build": "npm run delete && concurrently \"npm run type-check\" \"npm run lint\" \"npm run build:bff\" \"npm run build:client\" && npm run install-prod-packs && npm run copy-frontend-to-bff",
    "build:bff": "babel ./src/bff --extensions \".ts\" -d dist/bff --copy-files --ignore __tests__",
    "build:client": " webpack --config webpack/webpack.config.js  --env env=prod",
    "install-prod-packs": "cp package.json dist/bff/ && cp package-lock.json dist/bff/ && cd dist/bff && npm ci --production --ignore-scripts && cd .. && cd ..",
    "copy-frontend-to-bff": "cp -r dist/frontend dist/bff/public",
    "type-check": "tsc --noEmit",
    "lint": "eslint --fix \"./src/**/*.{js,jsx,ts,tsx}\"",
    "format": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "test": " jest --silent --runInBand",
    "test:parallel": "jest --silent ",
    "test-dev": " jest --coverage --watchAll",
    "delete": "del-cli --force ./dist"
  },
  "dependencies": {
    "abort-controller": "^3.0.0",
    "cookie-parser": "^1.4.5",
