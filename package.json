    "express-pino-logger": "^6.0.0",
    "express-validator": "^6.12.0",
    "http-errors": "^1.8.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "node-fetch": "^2.6.6",
    "pino": "^6.12.0",
    "pino-pretty": "^5.0.2"
  },
